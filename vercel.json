{
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "routes": [
    {
      "src": "/messages",
      "destination": "/messages"
    },
    {
      "src": "/browse",
      "destination": "/browse"
    },
    {
      "src": "/post-need",
      "destination": "/post-need"
    }
  ]
}
